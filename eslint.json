{
    "env": {
        "browser": true,
        "amd": true
    },

    "globals": {
        "nx": true,
        "jQuery": true,
        "console": true
    },

    "rules": {
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 1,
        "no-comma-dangle": 2,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-keys": 2,
        "no-empty": 1,
        "no-empty-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 1,
        "use-isnan": 2,
        "valid-jsdoc": 0,

        "block-scoped-var": 0,
        "complexity": [2, 10],
        "consistent-return": 0,
        "curly": [1, "all"],
        "dot-notation": 1,
        "eqeqeq": 1,
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-div-regex": 0,
        "no-else-return": 0,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 0,
        "no-extend-native": 0,
        "no-fallthrough": 0,
        "no-floating-decimal": 2,
        "no-implied-eval": 0,
        "no-labels": 0,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 1,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 0,
        "no-new-func": 0,
        "no-new-wrappers": 0,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 0,
        "no-script-url": 0,
        "no-self-compare": 2,
        "no-unused-expressions": 2,
        "no-warning-comments": 0,
        "no-with": 0,
        "no-yoda": 0,
        "radix": 0,
        "wrap-iife": [2, "any"],

        "no-global-strict": 2,
        "no-extra-strict": 2,
        "strict": 0,

        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-unused-vars": [1, "all"],
        "no-use-before-define": 2,

        "brace-style": 0,
        "camelcase": 1,
        "consistent-this": 0,
        "func-names": 0,
        "func-style": 0,
        "new-cap": 2,
        "new-parens": 0,
        "no-nested-ternary": 0,
        "no-array-constructor": 2,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-space-before-semi": 1,
        "no-ternary": 0,
        "no-underscore-dangle": 0,
        "no-wrap-func": 2,
        "quotes": 0,
        "quote-props": 0,
        "semi": [2, "always"],
        "sort-vars": 0,
        "space-in-brackets": 0,
        "space-infix-ops": 0,
        "space-return-throw-case": 2,
        "space-unary-word-ops": 2,
        "max-nested-callbacks": [2, 5],
        "one-var": 0,
        "wrap-regex": 0
    }
}
